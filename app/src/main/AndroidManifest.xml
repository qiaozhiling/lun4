<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qzl.cloudalbum">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CloudAlbum">
        <activity android:name=".activities.UploadActivity" />
        <activity
            android:name=".activities.UserDetailActivity"
            android:label="我的" />
        <activity android:name=".activities.PicActivity" />
        <activity
            android:name=".activities.RegisterActivity"
            android:label="用户注册" />
        <activity
            android:name=".activities.DownloadedActivity"
            android:label="下载列表" />
        <activity android:name=".activities.FileActivity" />
        <activity android:name=".activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>